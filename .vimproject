rbbt-image=/$PWD filter="*" {
 Rakefile
 Dockerfile
 README.md
 Vagrantfile
 bin=bin{
  build_rbbt_provision_sh.rb
  run_rbbt_docker.rb
 }
 etc=etc{
  infrastructure=infrastructure{
   default.yaml
  }
 }

 lib=lib{
  rbbt=rbbt{
   docker.rb
  }
 }
 share=share{
  provision_scripts=provision_scripts filter="*"{
   Dockerfile.R-packages
   R_packages.sh
   R_setup.sh
   bootstrap.sh
   gem_setup.sh
   hacks.sh
   perl_setup.sh
   python_setup.sh
   ruby_setup.sh
   tokyocabinet_setup.sh
   ubuntu_setup.sh
   slurm_loopback.sh
   user_setup.sh
   variables.sh
  }
 }
}
